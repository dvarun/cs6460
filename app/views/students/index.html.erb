<div class="ui one column grid">
  <div class="column">
    <table class="ui celled table">
      <thead>
        <tr>
          <th>First Name</th>
          <th>Last Name</th>
          <th>Edit</th>
        </tr>
      </thead>
      <tbody>
        <% for user in @users%>
          <tr>
            <td data-label="fname"><%= user.first_name %></td>
            <td data-label="lname"><%= user.last_name %></td>
            <td data-label="action"><%#= link_to "Provide Feedback", feedback_path(task.id), :class => "item"%> Edit</td>
          </tr>
        <%end%>
      </tbody>
    </table>
  </div>
</div>
